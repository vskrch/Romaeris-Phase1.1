{
  "info": {
    "name": "Todo"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "done": {
      "type": "boolean",
      "default": true,
      "required": true,
      "unique": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  },
  "connection": "default",
  "collectionName": "todos"
}
